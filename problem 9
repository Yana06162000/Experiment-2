#include <iostream>

using namespace std;
int main()
{
    int columns, rows, c, r;
    r = 1;
    c = 2;
    
    cout << "Number of row/s: ";
    cin >> rows;
    cout << "Number of column/s: ";
    cin >> columns;
    
    for (int r = 1; r <= rows; r++)
        {
            for (int c = 2; c <= columns; c++)
                {
                    cout << "*";
                }
            cout << "*\n";
    }
    return 0;
}
