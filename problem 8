#include <iostream>

using namespace std;
int main()
{

    int n;
    
    cout << "Please enter a number: ";
    cin >> n;
    cout << "\nMultiplication table of " << n << ": " << endl;

    cout << "1 * " << n << " = " << 1 * n << endl;
    cout << "2 * " << n << " = " << 2 * n << endl;
    cout << "3 * " << n << " = " << 3 * n << endl;
    cout << "4 * " << n << " = " << 4 * n << endl;
    cout << "5 * " << n << " = " << 5 * n << endl;
    cout << "6 * " << n << " = " << 6 * n << endl;
    cout << "7 * " << n << " = " << 7 * n << endl;
    cout << "8 * " << n << " = " << 8 * n << endl;
    cout << "9 * " << n << " = " << 9 * n << endl;
    cout << "10 * " << n << " = " << 10 * n << endl;
    cout << "\nThank you!\n";
    
    return 0;
}
